{{#each megalithicMonuments}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{name}}
    </h2>
    <h4 class="title is-size-5">
      Description:
    </h4>
    <p>
      {{description}}
    </p>
    <h3 class="title is-size-5">
      Location:
    </h3>
    <p>
      Lat: {{lat}}
    </p>
    <p>
      lng: {{lng}}
    </p>
    <a href="/dashboard/deletemegalithicmonument/{{_id}}" class="button">
    <span class="icon-text">
      <span class="icon has-text-danger">
        <i class="far fa-times-circle"></i>
      </span>
    <span>Delete</span>
    </span>
    </a>
  </div>
{{/each}}